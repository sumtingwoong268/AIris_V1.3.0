{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "dist",

  "rewrites": [
    { "source": "/((?!api).*)", "destination": "/" }
  ],
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": false }
  ]
}
